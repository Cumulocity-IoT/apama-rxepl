using utils.Test;
using utils.ExpectValues;

using com.industry.rx_epl.Observable;

monitor TestObservable {
	Test test := Test("TestResult");
  
	action onload() {
	    any value := Observable.empty()
			.getSyncOr("Done");
	    if value = <any> "Done" {
	    	test.complete();
	    } else {
	    	test.fail("Unexpected value");
	    }
	}  
}
