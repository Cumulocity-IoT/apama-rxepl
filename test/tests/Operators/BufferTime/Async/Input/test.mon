monitor TestObservable {
	Test test := Test("TestResult");
  
	integer teardownCount := 0;
	
	action onload() {
		on utils.KeepAliveUntilTerminated() {}
		any discard := Observable.interval(0.1)
			.take(7)
			.bufferTime(0.3)
